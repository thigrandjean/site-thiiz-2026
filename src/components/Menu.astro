---
const menu = [
  {label: "Dreams", href: "/dreams"},
  {label: "Work", href: "/work"}
]
---
<nav>
  <ul class="flex gap-4 text-white text-xl uppercase">
    {menu.map((item) => (
    <li>
      <a class="hover:text-yellow active:text-red transition-colors" href={item.href}>{item.label}</a>
    </li>
    ) )}
  </ul>
</nav>
